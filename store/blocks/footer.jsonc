  "flex-layout.col#footer-right-desktop": {
    "children": ["social-networks"]
  },
  "social-networks": {
    "socialNetworks": [
      {
        "url": "https://facebook.com/flatflat",
        "name": "facebook"
      },
      {
        "url": "https://instagram.com/flatflat",
        "name": "instagram"
      },
      {
        "url": "https://twitter.com/flatflat",
        "name": "twitter"
      }
    ],
    "showInColor": true
  }
